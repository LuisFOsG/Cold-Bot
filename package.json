{
  "name": "cold-bot",
  "version": "1.0.0",
  "description": "Discord Bot",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon src/index.js --exec babel-node",
    "build": "cross-env NODE_ENV=production babel src --out-dir dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
		"lint": "eslint --fix",
		"commit": "git-cz",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuisFOsG/Cold-Bot.git"
  },
  "author": "LuisFOsG",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/LuisFOsG/Cold-Bot/issues"
  },
  "homepage": "https://github.com/LuisFOsG/Cold-Bot#readme",
  "dependencies": {
    "@discordjs/rest": "0.1.0-canary.0",
    "ascii-table": "0.0.9",
    "canvas": "2.8.0",
    "core-js": "3.17.3",
    "cross-env": "7.0.3",
    "discord-api-types": "0.23.1",
    "discord.js": "13.1.0",
    "dotenv": "10.0.0",
    "figlet": "1.5.2",
    "moment": "2.29.1",
    "quick.db": "github:lorencerri/quick.db",
    "regenerator-runtime": "0.13.9"
  },
  "devDependencies": {
    "@babel/cli": "7.15.4",
    "@babel/core": "7.15.5",
    "@babel/node": "7.15.4",
    "@babel/preset-env": "7.15.6",
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@commitlint/cz-commitlint": "13.1.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "2.24.2",
    "husky": "7.0.0",
    "lint-staged": "11.1.2",
    "nodemon": "2.0.12"
  },
	"lint-staged": {
    "*.js": "npm run lint"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": "3"
        }
      ]
    ]
  },
  "eslintIgnore": [
    "node_modules",
    "dist",
    "public",
    "LICENSE"
  ]
}
